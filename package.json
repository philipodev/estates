{
  "name": "estates",
  "description": "a tiny (not global) state machine for react trees",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/estates.umd.cjs",
  "module": "./dist/estates.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/estates.js",
      "require": "./dist/estates.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "immer": "^9.0.15"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.0",
    "vitest": "^0.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/philipodev/estates.git"
  },
  "contributors": [
    {
      "name": "Philip Oliver",
      "email": "philipodev@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/philipodev/estates/issues"
  },
  "homepage": "https://github.com/philipodev/estates"
}
